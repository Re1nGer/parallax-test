---
import Layout from '../layouts/Layout.astro';
import { Image } from 'astro:assets'
import Landing from "../components/Landing.astro";
import Contacts from "../components/Contacts.astro";
import AboutMeMobile from "../components/AboutMeMobile.astro";
import Work from "../components/Work.astro";
import SideProjectsMobile from "../components/SideProjectsMobile.astro";
import ContactsMobile from "../components/ContactsMobile.astro";
import AboutMe from "../components/AboutMe.astro";
import WorkMobile from "../components/WorkMobile.astro";
import SideProjects from "../components/SideProjects.astro";

---

<Layout title="Welcome to Astro.">
